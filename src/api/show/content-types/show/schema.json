{
  "kind": "collectionType",
  "collectionName": "shows",
  "info": {
    "singularName": "show",
    "pluralName": "shows",
    "displayName": "Shows",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "ShowName": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "ShowSlug": {
      "type": "uid",
      "targetField": "ShowName",
      "required": true
    },
    "Broadcast_IsRepeat": {
      "type": "boolean"
    },
    "Broadcast_Day": {
      "type": "enumeration",
      "conditions": {
        "visible": {
          "==": [
            {
              "var": "Broadcast_IsRepeat"
            },
            true
          ]
        }
      },
      "enum": [
        "Monday",
        "Tuesday",
        "Wednesday",
        "Thursday",
        "Friday",
        "Saturday",
        "Sunday"
      ]
    },
    "Broadcast_Time": {
      "type": "integer",
      "conditions": {
        "visible": {
          "==": [
            {
              "var": "Broadcast_IsRepeat"
            },
            true
          ]
        }
      },
      "min": 1,
      "max": 12
    },
    "Broadcast_AmPm": {
      "type": "enumeration",
      "conditions": {
        "visible": {
          "==": [
            {
              "var": "Broadcast_IsRepeat"
            },
            true
          ]
        }
      },
      "enum": [
        "am",
        "pm"
      ]
    },
    "Broadcast_RepeatEvery_Weeks": {
      "type": "integer",
      "conditions": {
        "visible": {
          "==": [
            {
              "var": "Broadcast_IsRepeat"
            },
            true
          ]
        }
      },
      "default": 4
    },
    "ShowDescription": {
      "type": "blocks",
      "required": false
    },
    "ShowImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "StaffComments": {
      "type": "blocks",
      "private": true
    },
    "Show_Episodes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::episode.episode",
      "mappedBy": "link_episode_to_show"
    },
    "Main_Host": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::artist.artist",
      "mappedBy": "Main_host"
    },
    "Show_Instagram": {
      "type": "string"
    },
    "WebLink1": {
      "type": "string"
    },
    "WebLink2": {
      "type": "string"
    }
  }
}
